{
  "manifest_version": 2,
  "name": "Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["js/jquery-3.1.0.min.js", "js/main.js"],
      "css": ["css/styles.css"],
      "run_at": "document_end"
    },
    {
      "matches": ["http://*/*", "https://*/*", "file://*/*"],
      "js": ["js/jquery-3.1.0.min.js", "js/grammar.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["js/jquery-3.1.0.min.js", "js/SearchReverser.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["http://*/*", "https://*/*", "file://*/*"],
      "js": ["js/jquery-3.1.0.min.js", "js/hyperlinks.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["http://*/*", "https://*/*", "file://*/*"],
      "js": ["js/jquery-3.1.0.min.js", "js/comicsans.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["http://*/*", "https://*/*", "file://*/*"],
      "js": ["js/jquery-3.1.0.min.js", "js/righttoleft.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["http://*/*", "https://*/*", "file://*/*"],
      "js": ["js/jquery-3.1.0.min.js", "js/buildawall.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["js/jquery-3.1.0.min.js", "js/ruingoogle.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "Make Chrome Great Again",
    "default_icon": "appicon.png",
    "default_popup": "/interface/index.html"
	}
}
